contract Remote =
  entrypoint f : () => unit
contract Test =
  record r = { x : int, y : string, z : map(int, int) }
  datatype d('a) = One('a, 'a) | Two
  entrypoint a(x : int * bool * string * unit) = x
  entrypoint b(x : d(int) * d(int)) = x
  entrypoint c(x : r) = x
  entrypoint d(x : bytes(2) * bytes(14) * bytes(32) * bytes(65)) = x
  entrypoint e(x : oracle(int, int) * oracle_query(int, int) * Remote * address) = x
  entrypoint f(x : list(int)) = x
